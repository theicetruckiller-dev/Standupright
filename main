-- [10] Kısa yol: Sağ Frame_2'ye yeni panel açma butonları eklendi
local extraBtns = {
    {txt="Farm Items", panel=FarmPanel, ypos=0.034},
    {txt="Skill Spam", panel=SkillPanel, ypos=0.16},
    {txt="Webhook",    panel=WebhookPanel, ypos=0.29},
}
for _,v in ipairs(extraBtns) do
    local btn = Instance.new("TextButton")
    btn.Parent = Frame_2
    btn.Size = UDim2.new(1, 0, 0.1, 0)
    btn.Position = UDim2.new(0,0,v.ypos,0)
    btn.BackgroundColor3 = Color3.fromRGB(255,85,127)
    btn.Font = Enum.Font.Jura
    btn.Text = v.txt
    btn.TextColor3 = Color3.fromRGB(255,255,255)
    btn.TextScaled = true
    btn.MouseButton1Click:Connect(function()
        v.panel.Visible = not v.panel.Visible
    end)
end

-- [11] OTOMATİK SKILL SPAM (SkillPanel'den seçilenler)
spawn(function()
    while wait(0.35) do
        for sk,_ in pairs(skillSelected) do
            if skillSelected[sk] then
                local ev = game.ReplicatedStorage.Events:FindFirstChild(sk)
                if ev then
                    pcall(function() ev:FireServer() end)
                end
            end
        end
    end
end)

-- [12] BOSS ITEM WEBHOOK (isteğe bağlı, oyun içi boss drop kontrolü eklenebilir)
-- Boss item spawn olduğunda bildirim gönderme örneği:
spawn(function()
    while wait(3) do
        if webhookLink~="" and webhookOptions.bossItem then
            for _,itm in ipairs({"DIO's Diary","Heaven Arrow","Kars Mask"}) do
                if workspace.ItemSpawns:FindFirstChild(itm) then
                    local http = (syn and syn.request) or (http and http.request) or http_request or request
                    if http then
                        local money,level = "?", "?"
                        local ls = game.Players.LocalPlayer:FindFirstChild("leaderstats")
                        if ls then
                            if ls:FindFirstChild("Money") then money=ls.Money.Value end
                            if ls:FindFirstChild("Level") then level=ls.Level.Value end
                        end
                        http({
                            Url = webhookLink,
                            Method = "POST",
                            Headers={["Content-Type"]="application/json"},
                            Body=game:GetService("HttpService"):JSONEncode({
                                content = "@everyone\nKategori: Boss Item\nItem: "..itm.."\nOyuncu: "..game.Players.LocalPlayer.Name.."\nSaat: "..os.date("%X").."\nPara: "..money.."\nSeviye: "..level
                            })
                        })
                    end
                end
            end
        end
    end
end)

