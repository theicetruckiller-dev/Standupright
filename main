-- [6] DRAG SUPPORT (Frame ve Popuplar)
local UIS = game:GetService("UserInputService")
local function makeDraggable(gui)
    local dragging, dragInput, dragStart, startPos
    gui.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            dragging = true
            dragStart = input.Position
            startPos = gui.Position
            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then dragging = false end
            end)
        end
    end)
    gui.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
            dragInput = input
        end
    end)
    UIS.InputChanged:Connect(function(input)
        if input == dragInput and dragging then
            local delta = input.Position - dragStart
            gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
        end
    end)
end
makeDraggable(Frame)
makeDraggable(FarmPanel)
makeDraggable(SkillPanel)
makeDraggable(WebhookPanel)

-- [7] BUTON İŞLEVLERİ - KODLAR
-- Speed (WalkSpeed)
Frame.Speed.MouseButton1Click:Connect(function()
    getgenv().WalkSpeedValue = 50
    local Player = game:service'Players'.LocalPlayer
    Player.Character.Humanoid:GetPropertyChangedSignal'WalkSpeed':Connect(function()
        Player.Character.Humanoid.WalkSpeed = getgenv().WalkSpeedValue
    end)
    Player.Character.Humanoid.WalkSpeed = getgenv().WalkSpeedValue
end)
-- Farm Items (Popup aç, farm başlat)
Frame.Speed2.MouseButton1Click:Connect(function()
    FarmPanel.Visible = not FarmPanel.Visible
    itemPanelVisible = FarmPanel.Visible
end)
-- Sell Ketchup
Frame.Speed3.MouseButton1Click:Connect(function()
    while wait() do
        game.ReplicatedStorage.Events.SellKetchup:FireServer("Lots")
    end
end)
-- Backpack
Frame.Speed4.MouseButton1Click:Connect(function()
    local mouse = game.Players.LocalPlayer:GetMouse()
    local GUI = game.CoreGui
    local Back = GUI.RobloxGui.Backpack
    Back.Visible = true
    mouse.KeyDown:Connect(function(key)
        if key == "q" then
            Back.Inventory.Visible = not Back.Inventory.Visible
        end
    end)
end)
-- Auto M1 (SkillPanel aç, spam başlat)
Frame.Speed5.MouseButton1Click:Connect(function()
    if not skillPanelVisible then
        SkillPanel.Visible = true
        skillPanelVisible = true
        return
    end
    -- auto M1 + skill spam başlat
    while wait(0.22) do
        pcall(function()
            game:GetService("Players").LocalPlayer.Character.StandEvents.M1:FireServer()
            for sk,_ in pairs(skillSelected) do
                if skillSelected[sk] then
                    local ev = game.ReplicatedStorage.Events:FindFirstChild(sk)
                    if ev then ev:FireServer() end
                end
            end
        end)
    end
end)
-- Anti Afk
Frame.Speed6.MouseButton1Click:Connect(function()
    local vu = game:GetService("VirtualUser")
    game:GetService("Players").LocalPlayer.Idled:connect(function()
        vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
        wait(1)
        vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
    end)
end)
-- Rokaka
Frame.Speed7.MouseButton1Click:Connect(function()
    local Players = game:GetService("Players")
    local player = Players:FindFirstChildOfClass('Player')
    if player and player.Character then
        local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            local tool = player.Backpack:FindFirstChild("Rokakaka")
            if tool then humanoid:EquipTool(tool) end
        end
    end
    game:GetService("Players").LocalPlayer.Character.Rokakaka.Use:FireServer()
end)
