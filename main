            local humanoid = game.Players.LocalPlayer.Character.Humanoid
            humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
            wait(0.1)
            pl.CFrame = tp.c
        end
    end)
end

-- [8] OTOMATİK ITEM FARM (Seçili itemları sürekli al)
spawn(function()
    while wait(2) do
        for itm,_ in pairs(farmItemsSelected) do
            if farmItemsSelected[itm] then
                for _,v in pairs(workspace.ItemSpawns:GetChildren()) do
                    if v.Name == itm then
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
                        wait(0.15)
                        -- Webhook bildir (item)
                        if webhookLink~="" and webhookOptions.worldItem then
                            local http = (syn and syn.request) or (http and http.request) or http_request or request
                            if http then
                                local money,level = "?", "?"
                                local ls = game.Players.LocalPlayer:FindFirstChild("leaderstats")
                                if ls then
                                    if ls:FindFirstChild("Money") then money=ls.Money.Value end
                                    if ls:FindFirstChild("Level") then level=ls.Level.Value end
                                end
                                http({
                                    Url = webhookLink,
                                    Method = "POST",
                                    Headers={["Content-Type"]="application/json"},
                                    Body=game:GetService("HttpService"):JSONEncode({
                                        content = "@everyone\nKategori: World Item\nItem: "..itm.."\nOyuncu: "..game.Players.LocalPlayer.Name.."\nSaat: "..os.date("%X").."\nPara: "..money.."\nSeviye: "..level
                                    })
                                })
                            end
                        end
                    end
                end
            end
        end
    end
end)

-- [9] ESKİ Farm Items (Yerdeki tüm itemları farmla) -- isteğe bağlı, yenisiyle aynı anda çalışmaz!
